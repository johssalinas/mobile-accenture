    "android:emulate": "ionic cordova emulate android",
    "ios:build": "npm run config:env && ionic cordova build ios",
    "ios:build:prod": "npm run config:env:prod && ionic cordova build ios --prod --release",
    "ios:run": "ionic cordova run ios",
    "ios:emulate": "ionic cordova emulate ios",
    "cordova:prepare": "ionic cordova prepare",
    "cordova:platform:add:android": "ionic cordova platform add android",
    "cordova:platform:add:ios": "ionic cordova platform add ios",
    "release": "pwsh -File scripts/create-release.ps1",
    "release:bash": "bash scripts/create-release.sh",
    "build:ios:unsigned": "bash scripts/build-ios-unsigned.sh"
  },
  "private": true,
