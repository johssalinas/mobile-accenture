  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ionic serve --no-open",
    "serve": "ng serve",
    "serve:ionic": "ionic serve --no-open",
    "serve:open": "ionic serve",
    "config:env": "node scripts/set-env.js",
    "config:env:prod": "node scripts/set-env.js --prod",
    "prebuild": "npm run config:env",
